{
  "$schema": "https://raw.githubusercontent.com/raineorshine/npm-check-updates/main/src/types/RunOptions.json",
  "upgrade": true,
  "target": "minor",
  "install": "always",
  "packageManager": "pnpm",
  "reject": [
    "nx",
    "eslint",
    "@nx/*",
    "@swc/*",
    "@swc-node/*",
    "@reactive-resume/*",
    "eslint-plugin-unused-imports"
  ]
}
